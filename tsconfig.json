{
    "extends": "./.nuxt/tsconfig.json",
    "compilerOptions": {
        "moduleResolution": "Bundler",
        "allowImportingTsExtensions": true,
        "paths": {
            "@/*": [
                "./*"
            ],
            "~/*": [
                "./*"
            ]
        },
        "skipLibCheck": true,
        "typeRoots": [
            "./node_modules/@types",
        ]
    }
}
